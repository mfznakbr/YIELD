name: prediksi-tebu-mlflow

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 10}
      data_path: {type: string, default: "data_train.xlsx"}
    command: "python modeling.py {n_estimators} {max_depth} {data_path}"
